@model VidlyNet7.Models.Movie
@{
    ViewBag.Title = "Details";
    Layout = "_Layout";
}
<h1>@Model.Name</h1>

<ul>
    <li>Genre: @Model.Genre</li>
    <li>Release Date: @Model.ReleaseDate.ToString("d")</li>
    <li>Date Added: @Model.DateAdded.ToString("d")</li>
    <li>Number in Stock: @Model.Stock</li>
    
    
</ul>



    